{
  "name": "week-18",
  "private": true,
  "scripts": {
    "start": "node dist/index.js",
    "build": "turbo run build",
    "build:docs": "npx turbo run build --filter docs",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "clean": "turbo run clean",
